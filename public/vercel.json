{
  "version": 2,
  "builds": [
    { "src": "api/data.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/data",
      "dest": "/api/data.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
