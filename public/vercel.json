{
  "version": 2,
  "builds": [
    {
      "src": "api/data.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/data",
      "dest": "/api/data.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
